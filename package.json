{
  "name": "classlist-enhanced",
  "version": "2.0.0",
  "description": "An enhanced classlist for the html DOM",
  "main": "dist/bundle.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "ebam"
  },
  "keywords": [
    "dom,DOM,html,css,styles",
    "dom",
    "DOM",
    "html",
    "css",
    "styles"
  ],
  "author": "Quentin Engles <hollowdoor99@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "array-from": "^2.1.1",
    "dom-class-styles": "^1.0.0",
    "dom-classlist": "^1.0.1",
    "dom-eve": "^1.0.0",
    "dom-get-element": "^1.1.1",
    "more-events": "^2.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hollowdoor/classlist_enhanced.git"
  },
  "ebam": {
    "entry": "src/index.js",
    "transforms": {
      "dangerousForOf": false,
      "dangerousTaggedTemplateString": false
    },
    "test": {
      "dest": "test/code.js",
      "src": "test/src.js"
    }
  },
  "jsnext:main": "dist/bundle.es.js",
  "module": "dist/bundle.es.js"
}
